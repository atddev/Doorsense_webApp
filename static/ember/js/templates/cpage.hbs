  <div id="page-wrapper">

            <div class="container-fluid">

                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">
                            Metawear Logs
                        </h1>
                        <ol class="breadcrumb">
                            <li class="active">
                                <i class="fa fa-fw fa-desktop"></i>  <a href="/#/cpage">Door Logs</a>
                            </li>
                        </ol>
                    </div>
                </div>
                <!-- /.row -->
                
                    <div class="col-lg-8">
                        <h3>Last 10 logs</h3>
                        <div class="table-responsive">
                            <table class="table table-bordered table-hover table-striped">
                                <thead>
                                    <tr>
                                        <th data-align="center" > Event <span class="glyphicon glyphicon-user"> </span>  </th>
                                        <th data-align="center" > Time  <span class="glyphicon glyphicon-eye-open"> </span> </th>
                                        <th data-align="center"> Date <span class="glyphicon glyphicon-calendar"> </span> </th>
                                        <th> Server Time <span class="glyphicon glyphicon-time"></span> </th>
                                    </tr>
                                </thead>
                                <tbody>
                                     {{#each controller}}    
                                     {{#link-to "logg" id tagName='tr'}}                     
                                        <td><span class="badge">{{event}}</span></td> 
                                        <td>{{stime}}</td>  
                                        <td>{{vis_date}}</td>
                                        <td>{{vis_time}}</td>
                                        {{/link-to}}
                                     {{/each}}
                                </tbody>
                            </table>
                        </div>

                         <br><br>
                    </div>
                
                <!-- /.row -->


            </div>
            <!-- /.container-fluid -->

        </div>
        <!-- /#page-wrapper -->

   
    <!-- /#wrapper -->

    <!-- jQuery -->
    <script src="../static/js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="static/js/bootstrap.min.js"></script>


