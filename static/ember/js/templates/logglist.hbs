      {{render "auth"}}

        <div id="page-wrapper">

            <div class="container-fluid">

                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">
                           Logs
                        </h1>
                        <ol class="breadcrumb">
                            <li>
                                <i class="fa fa-fw fa-desktop"></i>  <a href="/#/cpage">Control Panel</a>
                            </li>
                            <li class="active">
                                <i class="fa fa-list"></i> Logs
                            </li>
                        </ol>
                    </div>
                </div>

                <!-- /.start TEST table -->

<div class="col-sm-8">
<span class="counter pull-right"></span>
     <table class="table table-bordered table-hover table-striped" data-toggle="table" data-pagination="true" data-search="true">
                                <thead>
                                    <tr>
                                        <th data-align="center" > Event <span class="glyphicon glyphicon-user"> </span>  </th>
                                        <th data-align="center" > Time  <span class="glyphicon glyphicon-eye-open"> </span> </th>
                                        <th data-align="center"> Date <span class="glyphicon glyphicon-calendar"> </span> </th>
                                        <th> Server Time <span class="glyphicon glyphicon-time"></span> </th>
                                    </tr>
                                </thead>
                                <tbody>
                                     {{#each controller}}    
                                     {{#link-to "logg" id tagName='tr'}}                     
                                        <td><span class="badge">{{event}}</span></td> 
                                        <td>{{stime}}</td>  
                                        <td>{{vis_date}}</td>
                                        <td>{{vis_time}}</td>
                                        {{/link-to}}
                                     {{/each}}
                                </tbody>
                            </table>
                            <br><br>
</div>


            </div>
            <!-- /.container-fluid -->

        </div>
        <!-- /#page-wrapper -->

		
		

    <!-- jQuery -->
    <script src="../static/js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="static/js/bootstrap.min.js"></script>


